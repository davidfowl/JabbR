@using JabbR.Services
@{
    Layout = "_layout.cshtml";
}
@switch ((AuthenticationMode)Model)
{
    case AuthenticationMode.Default:
    <div class="row-fluid">
        <div class="span5">
            @Html.Partial("_username")
        </div>
        <div class="span5">
            @Html.Partial("_social")
        </div>
    </div>
        break;
    case AuthenticationMode.Windows:
    @Html.Partial("_windows")
        break;
}
